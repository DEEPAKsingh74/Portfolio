{
	"name": "Portfolio",
	"dockerComposeFile": "docker-compose-dev.yml",
	"service": "api-gateway",
	"workspaceFolder": "/app",
	"forwardPorts": [
		3000,
		5001,
		8002,
		3001
	],
	"postCreateCommand": "npm install && echo 'Post-create commands executed.'",
	"remoteEnv": {
		"PREDICTION_SERVICE_PORT": "5001",
		"PREDICTION_SERVICE_PORT_INTERNAL": "5000",
		"NGINX_SERVICE_PORT": "80",
		"NGINX_SERVICE_PORT_INTERNAL": "80",
		"API_GATEWAY_SERVICE_PORT": "3000",
		"API_GATEWAY_SERVICE_PORT_INTERNAL": "3000",
		"FRONTEND_SERVICE_PORT": "8002",
		"FRONTEND_SERVICE_PORT_INTERNAL": "8000",
		"COMMENT_TOXICITY_SERVICE_PORT": "3001",
		"COMMENT_TOXICITY_SERVICE_PORT_INTERNAL": "3001"
	},
	"remoteUser": "vscode"
}